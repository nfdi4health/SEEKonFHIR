<h1>New  <%= WorkflowClass.model_name.human %></h1>

<%= form_for(@workflow_class) do |f| %>
  <%= f.error_messages %>

  <%= render partial: 'workflow_classes/workflow_class_fields', locals: { f: f } %>

  <div>
    <%= submit_tag "Create", data: { disable_with: 'Creating...' }, class: 'btn btn-primary' %>
    or <%= cancel_button workflow_classes_path -%>
  </div>
<% end -%>
